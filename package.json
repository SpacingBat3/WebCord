{
  "name": "electron-discord-webapp",
  "productName": "Electron Discord WebApp",
  "version": "0.2.0-rc2",
  "description": "A Discord Web App based on the Electron Engine.",
  "main": "js/main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "build-linux": "electron-builder -l tar.gz --arm64 --armv7l --x64 --ia32"
  },
  "keywords": [
    "discord",
    "webapp",
    "electron"
  ],
  "author": "SpacingBat3",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SpacingBat3/electron-discord-webapp"
  },
  "bugs": {
    "url": "https://github.com/SpacingBat3/electron-discord-webapp/issues"
  },
  "homepage": "https://github.com/SpacingBat3/electron-discord-webapp#readme",
  "build": {
    "appId": "com.spacingbat3.webapp-discord-electron",
    "linux": {
      "target": [
        "pacman",
        "snap",
        "deb"
      ],
      "desktop": {
        "Name": "Discord",
        "Icon": "discord",
        "Comment": "Electron Web App for Discord.",
        "Categories": [
          "Network",
          "discord",
          "electron",
          "web"
        ]
      },
      "category": "Network"
    },
    "extraResources": [
      {
        "from": "icons/app.png",
        "to": "app.png"
      }
    ],
    "extraFiles": [
      "LICENSE.md"
    ]
  },
  "devDependencies": {
    "electron": "^10.1.5",
    "electron-builder": "^22.9.1"
  },
  "dependencies": {
    "deepmerge": "^4.2.2",
    "electron-json-config": "^1.5.3"
  }
}
