    "electron-fetch": "^1.7.3",
    "highlight.js": "^11.4.0",
    "marked": "^4.0.12",
    "semver": "^7.3.5",
    "source-map-support": "^0.5.21",
    "spdx-expression-parse": "^3.0.1",
    "tslib": "^2.3.1",
    "twemoji-colr-font": "^14.0.2",
    "ws": "^8.5.0"
